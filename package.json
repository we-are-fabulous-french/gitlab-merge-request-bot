{
  "name": "@discord-factory/factory",
  "version": "1.0.0",
  "description": "",
  "types": "contracts/**/*.d.ts",
  "author": "Baptiste Parmantier <baptiste.parmantier.pro@gmail.com>",
  "license": "MIT",
  "repository": "git@github.com:DiscordFactory/Factory.git",
  "keywords": [],
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --quiet --transpile-only --clear --rs -- src/index.ts",
    "start": "cross-env NODE_ENV=production node ./build/start/index.js",
    "factory": "ts-node-dev --quiet --transpile-only -- factory.ts",
    "build": "tsc",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "ava"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "ava": "^3.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-plugin-ava": "^12.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "discord.js": "13.1.0",
    "module-alias": "^2.2.2",
    "winston": "^3.3.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "test/**/*.spec.ts"
    ]
  },
  "_moduleAliases": {
    "src": "src"
  }
}
